    "license": "MIT",
    "dependencies": {
        "@mui/material": "^5.15.10",
        "@mui/x-date-pickers": "^6.19.4",
        "express": "^4.18.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.22.0",
        "react-scripts": "5.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.23.6",
        "@babel/preset-env": "^7.23.6",
        "@babel/preset-react": "^7.23.3",
        "babel-loader": "^9.1.3",
        "concurrently": "^6.0.2",
        "cross-env": "^7.0.3",
        "css-loader": "^6.8.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.6.0",
        "node-sass": "^9.0.0",
        "nodemon": "^2.0.7",
        "style-loader": "^3.3.3",
        "url-loader": "^4.1.1",
        "webpack": "^5.89.0",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.15.1"
    }
